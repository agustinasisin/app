<div id="<%= dom_id payment %>">

<td>
    <% payment.appointments.each do |appointment| %>
        <%= appointment.patient.full_name %>
    <% end %>
</td>
<td><%= payment.payment_method unless payment.payment_method.nil? %></td>
<td><%= payment.amount unless payment.amount.nil? %></td>
<td><%= payment.datetime unless payment.datetime.nil? %></td>

</div>
